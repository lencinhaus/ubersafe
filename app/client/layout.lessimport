#flash-messages {
  position: fixed;
  bottom: 0px;
  left: 0px;
  z-index: @zindex-modal-background + 1;
  margin-top: 60px;
  width: 100%;
}

#nprogress {
  .bar, .spinner {
    z-index: @zindex-navbar-fixed + 1;
  }
}

#modal-public-key .modal-dialog {
  width: 300px;
}

#public-key {
  font-family: @font-family-monospace;
  line-height: @line-height-base;
  width: auto;
  margin: 0 auto;
  overflow: hidden;
}